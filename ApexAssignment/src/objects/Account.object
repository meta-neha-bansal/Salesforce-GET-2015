    <validationRules>
        <fullName>Account_Number_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISBLANK(AccountNumber)),NOT(ISNUMBER(AccountNumber)) )</errorConditionFormula>
        <errorMessage>Account number must be a number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Postal_Code_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(BillingCountry = &quot;USA&quot;, BillingCountry = &quot;US&quot;),
NOT(REGEX(BillingPostalCode, &quot;\\d{5}(-\\d{4})?&quot;))
)</errorConditionFormula>
        <errorMessage>Billing postal code must be in format 99999 OR 99999-9999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CreditCardNumberValidationRule</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( REGEX(  CreditCardNumber__c  , &quot;(((\\d{4}-){3}\\d{4})|\\d{16})?&quot;))</errorConditionFormula>
        <errorMessage>Credit_Card_Number should be formatted in 9999-9999-9999-9999 or 9999999999999999 number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Max_PCT_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(AND(  Max_PCT__c &lt;=100, Max_PCT__c  &gt;=0))</errorConditionFormula>
        <errorMessage>MaxPCT can be between 0-100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multiple_Of_Five_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>MOD( Multiple_Of_Five__c ,5) &lt;&gt; 0</errorConditionFormula>
        <errorMessage>Number should be a multiple of 5</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dateValidationRule</fullName>
        <active>true</active>
        <errorConditionFormula>CASE(MOD(  date__c  - DATE(1900, 1, 7), 7),
0, 0,
6, 0,
1) = 0</errorConditionFormula>
        <errorMessage>date should be a weekday</errorMessage>
    </validationRules>
